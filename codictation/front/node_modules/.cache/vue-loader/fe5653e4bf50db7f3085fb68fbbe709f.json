;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAkMA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA","file":"profileSideBar.vue","sourceRoot":"src/components/profile/components","sourcesContent":["<template>\n  <div>\n    <Header></Header>\n    <q-separator />\n    <div class=\"content-box\">\n      <q-splitter v-model=\"splitterModel\">\n        <template v-slot:before>\n          <q-tabs v-model=\"innerTab\" vertical class=\"text-purple-9 tab-box\">\n            <q-img style=\"height: 200px\">\n              <div class=\"absolute-center bg-transparent\">\n                <div class=\"avatar-box\">\n                  <avatar-org class=\"side-avatar\"></avatar-org>\n                </div>\n                <!-- <avataaars></avataaars> -->\n                <!-- <img :src=\"imgstring\" width=\"67px\" height=\"67px\"> -->\n                <!-- <avatar :username=\"name\" class=\"avatar-box\"></avatar> -->\n                <div class=\"text-subtitle1 account-name text-center\">\n                  {{ name }}\n                </div>\n                <div\n                  class=\"text-body2 account-identity text-center text-weight-light\"\n                >\n                  「 {{ identity }} 」\n                </div>\n              </div>\n            </q-img>\n            <q-separator insert class=\"q-my-sm\" />\n            <q-tab\n              name=\"innerPosts\"\n              icon=\"assignment\"\n              label=\"Tasks\"\n              v-if=\"showPosts\"\n            />\n\n            <q-tab\n              name=\"innerTasks\"\n              icon=\"assignment\"\n              label=\"Tasks\"\n              v-if=\"showTasks\"\n            />\n            <q-tab \n              name=\"innerComments\" \n              icon=\"question_answer\" \n              label=\"Comments\"\n            />\n            <q-tab \n              name=\"innerBadges\" \n              icon=\"verified\" \n              label=\"Achievement\" \n            />\n            <q-tab \n              name=\"innerMistakes\"\n              icon=\"event_note\" \n              label=\"Mistakes\" \n            />\n\n            <q-separator insert class=\"q-my-sm\" />\n\n            <q-tab\n              name=\"innerNewTask\"\n              icon=\"add_task\"\n              label=\"New Task\"\n              v-if=\"isNewTask\"\n            />\n            <q-tab\n              name=\"innerAddUsers\"\n              icon=\"group_add\"\n              label=\"Add Users\"\n              v-if=\"isNewUser\"\n            />\n          </q-tabs>\n        </template>\n\n        <template v-slot:after>\n          <q-tab-panels\n            v-model=\"innerTab\"\n            animated\n            transition-prev=\"slide-down\"\n            transition-next=\"slide-up\"\n          >\n            <q-tab-panel name=\"innerPosts\">\n              <my-posts></my-posts>\n            </q-tab-panel>\n\n            <q-tab-panel name=\"innerTasks\">\n              <my-tasks></my-tasks>\n            </q-tab-panel>\n\n            <q-tab-panel name=\"innerComments\">\n              <my-comments></my-comments>\n            </q-tab-panel>\n\n            <q-tab-panel name=\"innerBadges\">\n              <my-badges></my-badges>\n            </q-tab-panel>\n\n            <q-tab-panel name=\"innerMistakes\">\n              <my-mistakes></my-mistakes>\n            </q-tab-panel>\n\n            <q-tab-panel name=\"innerNewTask\">\n              <new-task></new-task>\n            </q-tab-panel>\n\n            <q-tab-panel name=\"innerAddUsers\">\n              <add-users></add-users>\n            </q-tab-panel>\n          </q-tab-panels>\n        </template>\n      </q-splitter>\n    </div>\n  </div>\n</template>\n\n<script>\nimport AvatarOrg from \"../../basic/Avatar\";\n// import Avataaars from 'vuejs-avataaars';\n// import { generateFromString } from \"generate-avatar\";\nimport Header from \"../../basic/Header.vue\";\n// import Avatar from \"vue-avatar\";\nimport NewTask from \"../task\";\nimport AddUsers from \"../addUsers\";\nimport MyTasks from \"../mytasks\";\nimport MyPosts from \"../published\";\nimport MyComments from \"../mycomments\";\nimport MyBadges from \"../mybadges\";\nimport MyMistakes from \"../mymistakes\";\n\nexport default {\n  data() {\n    return {\n      name: \"\", //当前用户的用户名\n      userId: \"\", //当前用户的_id  可根据这两个信息去数据库User取数据\n      api: \"/api\",\n      tab: \"profile\",\n      innerTab: \"\",\n      splitterModel: 20,\n      status: \"0\",\n      identity: \"普通用户\",\n      isNewTask: false,\n      isNewUser: false,\n      showTasks: false,\n      showPosts: false,\n      // imgstring: \"\",\n    };\n  },\n  components: {\n    Header,\n    // Avatar,\n    NewTask,\n    AddUsers,\n    MyTasks,\n    MyPosts,\n    MyComments,\n    // generateFromString,\n    // Avataaars,\n    AvatarOrg,\n    MyBadges,\n    MyMistakes,\n  },\n  created() {\n    this.name = sessionStorage.getItem(\"userName\");\n    // this.imgstring = \"data:image/svg+xml;utf8,\" + generateFromString(this.name);\n    // console.log(\"imgstring\", this.imgstring);\n    this.$axios.get(this.api + \"/profile?name=\" + this.name).then((res) => {\n      this.userId = res.data[0]._id; //找地方保存\n      this.status = res.data[0].status;\n      if (this.status == \"0\") {\n        this.identity = \"普通用户\";\n        this.showTasks = true;\n        this.innerTab = \"innerTasks\";\n      }\n      if (this.status == \"1\") {\n        this.identity = \"老师\";\n        this.isNewTask = true;\n        this.isNewUser = true;\n        this.showPosts = true;\n        this.innerTab = \"innerPosts\";\n      }\n      if (this.status == \"2\") {\n        this.identity = \"组长\";\n        this.isNewTask = true;\n        this.showTasks = true;\n        this.showPosts = true;\n        this.innerTab = \"innerPosts\";\n      }\n      console.log(\"userId\", this.userId);\n      console.log(this.status);\n    });\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.side-avatar {\n  width: 4em;\n  height: 4em;\n}\n\n.absolute-center.bg-transparent {\n  width: 95%;\n}\n\n.content-box {\n  position: relative;\n  top: 4.4em !important;\n}\n\n.row {\n  display: flex;\n  flex-wrap: wrap;\n  margin-right: 0px;\n  margin-left: 0px;\n}\n\n.account-name {\n  color: black;\n  font-family: \"Prata\";\n  font-size: 20px;\n  margin-top: 0.6em;\n  font-weight: bold;\n}\n\n.account-identity {\n  color: black;\n  font-size: 17px;\n  margin-top: 0.6em;\n}\n\n.avatar-box {\n  text-align: center;\n}\n\n.absolute-center {\n  margin-top: 6px;\n}\n\n.tab-box {\n  position: fixed;\n  width: 19%;\n}\n\n.column {\n  flex-direction: row;\n}\n\n.q-tab__content {\n  width: 100%;\n}\n\n.q-tab__icon {\n  position: absolute;\n  left: 0;\n  margin: 0 10%;\n}\n\n.q-tab__label {\n  margin-left: 25px;\n}\n\n.q-splitter__panel.q-splitter__before {\n  width: 19% !important;\n}\n\n.q-tab--full {\n  min-height: 56px;\n}\n\n.q-splitter__separator {\n  min-height: 650px;\n}\n</style>\n"]}]}