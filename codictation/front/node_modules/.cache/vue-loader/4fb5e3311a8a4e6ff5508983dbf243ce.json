rgin: 0;\n  padding-top: 10px;\n  opacity: 1;\n  transform: translate(0);\n  transition: all 300ms linear;\n  transition-delay: 1500ms;\n  color: #7b617f;\n  font-family: sohne, \"Helvetica Neue\", Helvetica, Arial, sans-serif;\n}\n\n.subtitle {\n  position: relative;\n  text-align: center;\n  text-transform: uppercase;\n  letter-spacing: 2.5px;\n  font-weight: 700;\n  font-size: 14px;\n  color: #671067;\n}\n\n.subtitle:before {\n  margin-left: -68px;\n  text-align: right;\n}\n\n.subtitle:after {\n  margin-left: 15px;\n}\n\n.subtitle:before,\n.subtitle:after {\n  position: absolute;\n  top: 50%;\n  overflow: hidden;\n  width: 50px;\n  height: 1px;\n  border-top: 2px solid #cab3ce;\n  content: \"\";\n}\n\n.button {\n  font-family: \"Roboto Mono\", sans-serif;\n  text-decoration: none;\n  font-weight: 800;\n  text-transform: uppercase;\n  font-size: 2vmin;\n  display: inline-block;\n  position: relative;\n  border: 3px solid purple;\n  box-shadow: -0.5vmin 0.5vmin 0 rgba(purple, 0.5);\n  background: transparent;\n  margin-top: 7vmin;\n  mix-blend-mode: multiply;\n  color: purple;\n  padding: 2vmin 2vmin 1.8vmin 2vmin;\n  letter-spacing: 0.1em;\n  text-shadow: none;\n  line-height: 1;\n  transform: translate3d(0.5vmin, -0.5vmin, 0);\n  transition: all 100ms linear;\n\n  &:hover {\n    transform: translate3d(1vmin, -1vmin, 0);\n    box-shadow: -1vmin 1vmin 0 rgba(rgb(184, 95, 184), 0.5);\n    border: 3px solid rgb(185, 114, 185);\n    background: rgb(185, 114, 185);\n    color: white;\n  }\n\n  &:active {\n    transform: translate3d(0px, 0px, 0);\n    box-shadow: -0px 0px 0 rgba(purple, 0.5);\n  }\n}\n// 向下滑动提示动画\n.scroll-anima span {\n  position: absolute;\n  top: 68%;\n  left: 50%;\n  width: 24px;\n  height: 24px;\n  margin-left: -12px;\n  border-left: 1px solid #862897;\n  border-bottom: 1px solid #862897;\n  -webkit-transform: rotateZ(-45deg);\n  transform: rotateZ(-45deg);\n  -webkit-animation: sdb 1.5s infinite;\n  animation: sdb 1.5s infinite;\n  box-sizing: border-box;\n}\n@-webkit-keyframes sdb {\n  0% {\n    -webkit-transform: rotateY(0) rotateZ(-45deg) translate(0, 0);\n    opacity: 0;\n  }\n  50% {\n    opacity: 1;\n  }\n  100% {\n    -webkit-transform: rotateY(720deg) rotateZ(-45deg) translate(-20px, 20px);\n    opacity: 0;\n  }\n}\n@keyframes sdb {\n  0% {\n    transform: rotateY(0) rotateZ(-45deg) translate(0, 0);\n    opacity: 0;\n  }\n  50% {\n    opacity: 1;\n  }\n  100% {\n    transform: rotateY(720deg) rotateZ(-45deg) translate(-20px, 20px);\n    opacity: 0;\n  }\n}\n\n// 第二个section的css\n.intro-container {\n  height: 100%;\n  width: 100%;\n  padding: 1% 10%;\n  box-shadow: inset 6em 16em 25em -20em #560268;\n}\n.ipsType_right {\n  text-align: right;\n}\n.ipsType_center {\n  text-align: center;\n}\n.ipsSpacer_bottom_double {\n  margin-bottom: 3em;\n}\n.ipsGrid {\n  display: inline-block;\n  display: -ms-flexbox;\n  display: flex;\n  flex-wrap: wrap;\n}\n.ipsGrid::before,\n.ipsGrid::after {\n  display: table;\n  content: \"\";\n  line-height: 0;\n}\n.ipsGrid > [class*=\"ipsGrid_span\"] {\n  display: block;\n  width: 100%;\n  min-height: 30px;\n  box-sizing: border-box;\n}\n.ipsGrid > .ipsGrid_span5 {\n  width: 60%;\n}\n.ipsGrid > .ipsGrid_span7 {\n  width: 38%;\n}\n.ipsGrid > [class*=\"ipsGrid_span\"] {\n  float: left;\n  margin-left: 2%;\n}\n.ipsGrid > [class*=\"ipsGrid_span\"]:first-child {\n  margin-left: 0;\n}\n.feature {\n  display: flex;\n  align-items: center;\n}\nh2.heading_large {\n  font-size: 2em;\n  font-family: gt-super, Georgia, Cambria, \"Times New Roman\", Times, serif;\n}\nh6.gs_reveal {\n  font-family: gt-super, Georgia, Cambria, \"Times New Roman\", Times, serif;\n}\n\n// 最后一个section的wave背景\n@keyframes move_wave {\n  0% {\n    transform: translateX(0) translateZ(0) scaleY(1);\n  }\n  50% {\n    transform: translateX(-25%) translateZ(0) scaleY(0.55);\n  }\n  100% {\n    transform: translateX(-50%) translateZ(0) scaleY(1);\n  }\n}\n\n.footer {\n  width: 100%;\n  position: relative;\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));\n  margin: auto;\n  place-items: center;\n  min-height: 30vh !important;\n  padding: 50px 10vw;\n  margin: auto;\n}\n\n.footer-container {\n  display: flex;\n  position: relative;\n  width: 100vw;\n  max-height: 100vh;\n  overflow: hidden;\n\n  &::before {\n    content: \"\";\n    display: block;\n    padding-bottom: calc(100% / (16 / 9));\n  }\n}\n.waveWrapper {\n  overflow: hidden;\n  position: absolute;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  top: 0;\n  margin: auto;\n}\n.waveWrapperInner {\n  position: absolute;\n  width: 100%;\n  overflow: hidden;\n  height: 100%;\n  bottom: -3em;\n  background-image: linear-gradient(to top, #7c0f8f, #ffffff);\n}\n.bgTop {\n  z-index: 15;\n  opacity: 0.5;\n}\n.bgMiddle {\n  z-index: 10;\n  opacity: 0.75;\n}\n.bgBottom {\n  z-index: 5;\n}\n.wave {\n  position: absolute;\n  bottom: 3em;\n  left: 0;\n  width: 200%;\n  height: 100%;\n  background-repeat: repeat no-repeat;\n  background-position: 0 bottom;\n  transform-origin: center bottom;\n}\n.waveTop {\n  background-size: 50% 100px;\n}\n.waveAnimation .waveTop {\n  animation: move-wave 3s;\n  -webkit-animation: move-wave 3s;\n  -webkit-animation-delay: 1s;\n  animation-delay: 1s;\n}\n.waveMiddle {\n  background-size: 50% 120px;\n}\n.waveAnimation .waveMiddle {\n  animation: move_wave 10s linear infinite;\n}\n.waveBottom {\n  background-size: 50% 100px;\n}\n.waveAnimation .waveBottom {\n  animation: move_wave 15s linear infinite;\n}\n\n.home-sub-title {\n  color: #253439;\n  font-family: gt-super, Georgia, Cambria, \"Times New Roman\", Times, serif;\n}\n\n// 最后一个section的slogan\n.ml6 {\n  position: relative;\n  font-size: 6em;\n  color: #2b242a;\n}\n\n.ml6 .text-wrapper {\n  position: relative;\n  display: inline-block;\n  padding-top: 0.2em;\n  padding-right: 0.05em;\n  padding-bottom: 0.1em;\n  overflow: hidden;\n}\n\n.ml6 .letter {\n  display: inline-block;\n  line-height: 1.2em;\n}\n\n.footer-abs {\n  z-inde