AAA,MAAA,CACA,GADA,CACA,KAAA,GAAA,GAAA,iBAAA,GAAA,KAAA,SADA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,YAAA,OAAA,GAAA,GAAA,CAAA,IAAA,CADA,CAEA;;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,OAAA,CAAA,GAAA,CAAA,UAAA,IAAA,EAAA;AACA,iBAAA,IAAA,CAAA,OAAA;AACA,SAFA,CAAA,CAHA,CAMA;AACA,OATA;AAUA,KAvBA;AAwBA,IAAA,SAxBA,uBAwBA;AAAA;;AACA;AACA,WAAA,MAAA,CACA,GADA,CACA,KAAA,GAAA,GAAA,gBAAA,GAAA,KAAA,SADA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,GAAA,CAAA,IAAA,CAAA,CAAA,EAAA,MAAA;AACA,OAJA;AAKA,KA/BA;AAgCA,IAAA,YAhCA,0BAgCA;AACA,WAAA,QAAA,GAAA,EAAA,CADA,CACA;AACA,KAlCA;AAmCA;AACA,IAAA,WApCA,uBAoCA,GApCA,EAoCA,IApCA,EAoCA;AACA,UAAA,GAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,YAAA,CAAA,aAAA,CAAA,QAAA,CAAA,GAAA,CAAA,EAAA;AACA,UAAA,aAAA,CAAA,IAAA,CAAA,GAAA;AACA;;AACA,QAAA,IAAA,CAAA,GAAA,EAAA,QAAA,CAAA;AACA;AACA,KA3CA;AA4CA;AACA,IAAA,QA7CA,oBA6CA,GA7CA,EA6CA,MA7CA,EA6CA;AAAA;;AACA,MAAA,MAAA,CAAA,YAAA;AACA,YAAA,GAAA,KAAA,EAAA,EAAA;AACA,UAAA,MAAA,CAAA,aAAA,GAAA,aAAA;AACA,SAFA,MAEA;AACA,cAAA,MAAA,GAAA,GAAA,CAAA,WAAA,EAAA;AACA,UAAA,MAAA,CAAA,aAAA,GAAA,aAAA,CAAA,MAAA,CACA,UAAA,CAAA;AAAA,mBAAA,CAAA,CAAA,WAAA,GAAA,OAAA,CAAA,MAAA,IAAA,CAAA,CAAA;AAAA,WADA,CAAA;AAGA;AACA,OATA,CAAA;AAUA,KAxDA;AAyDA;AACA,IAAA,QA1DA,oBA0DA,GA1DA,EA0DA;AAAA;;AACA,UAAA,KAAA,GAAA,IAAA;;AACA,UAAA,cAAA,GAAA,KAAA,SAAA,CAAA,OAAA,CACA,KAAA,SAAA,CAAA,IAAA,CAAA,UAAA,OAAA;AAAA,eAAA,OAAA,IAAA,MAAA,CAAA,KAAA;AAAA,OAAA,CADA,CAAA;;AAGA,UAAA,cAAA,IAAA,CAAA,CAAA,IAAA,KAAA,KAAA,IAAA,EAAA,EAAA;AACA,QAAA,KAAA,CAAA,EAAA,CAAA,MAAA,CAAA;AACA,UAAA,KAAA,EAAA,OADA;AAEA,UAAA,SAAA,EAAA,OAFA;AAGA,UAAA,OAAA,EAAA,IAHA;AAIA,UAAA,IAAA,EAAA,6BAJA;AAKA,UAAA,OAAA,EAAA;AALA,SAAA;AAOA,OARA,MAQA;AACA,aAAA,UAAA,GAAA,KAAA;AACA,QAAA,GAAA,CAAA,MAAA,CAAA,MAAA,GAFA,CAGA;;AACA,aAAA,EAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,UAAA,OAAA,EAAA,gBADA;AAEA,UAAA,YAAA,EAAA,QAFA;AAGA,UAAA,WAAA,EAAA,EAHA;AAIA,UAAA,eAAA,EAAA;AAJA,SAAA,EAJA,CAUA;;AACA,aAAA,KAAA,GAAA,UAAA,CAAA,YAAA;AACA,UAAA,MAAA,CAAA,EAAA,CAAA,OAAA,CAAA,IAAA;;AACA,UAAA,MAAA,CAAA,KAAA,GAAA,KAAA,CAAA;AACA,SAHA,EAGA,IAHA,CAAA;AAIA;AACA,KAvFA;AAwFA;AACA,IAAA,aAzFA,2BAyFA;AACA,UAAA,KAAA,KAAA,KAAA,KAAA,CAAA,EAAA;AACA,QAAA,YAAA,CAAA,KAAA,KAAA,CAAA;AACA,aAAA,EAAA,CAAA,OAAA,CAAA,IAAA;AACA;AACA;AA9FA,GAlCA;AAkIA;AACA,EAAA,OAnIA,qBAmIA;AACA,QAAA,SAAA,GAAA,QAAA,CAAA,aAAA,CACA,6YADA,CAAA;AAGA,IAAA,SAAA,CAAA,SAAA,GAAA,SAAA,CAAA,SAAA,CAAA,OAAA,CAAA,UAAA,EAAA,EAAA,CAAA;AACA,GAxIA;AAyIA,EAAA,OAzIA,qBAyIA;AACA,SAAA,SAAA,GAAA,cAAA,CAAA,OAAA,CAAA,UAAA,CAAA;AACA,SAAA,YAAA;AACA,SAAA,UAAA;AACA,SAAA,SAAA;AACA;AA9IA,CAAA","sourcesContent":["<template>\n  <div class=\"q-px-md q-gutter-y-md row justify-start\">\n    <q-form\n      action=\"http://localhost:3000/api/task\"\n      method=\"post\"\n      enctype=\"multipart/form-data\"\n      class=\"q-gutter-sm col-7\"\n      @submit=\"onSubmit\"\n    >\n      <h4 class=\"q-my-lg\">发布任务</h4>\n\n      <!-- 当前发布者信息：只读 -->\n      <q-input\n        filled\n        v-model=\"publisher\"\n        hint=\"当前用户\"\n        readonly\n        label=\"发布者 *\"\n        name=\"publisher\"\n        dense\n      >\n        <template v-slot:before>\n          <q-icon name=\"person\" size=\"lg\" />\n        </template>\n      </q-input>\n\n      <!-- 上传音视频文件 -->\n      <q-file\n        clearable\n        clear-icon=\"close\"\n        standout=\"bg-purple-5 text-white\"\n        bottom-slots\n        v-model=\"fileName\"\n        label=\"请选择文件\"\n        counter\n        max-files=\"1\"\n        name=\"f1\"\n        accept=\"video/*,audio/*\"\n        dense\n      >\n        <template v-slot:before>\n          <q-icon name=\"create_new_folder\" size=\"lg\" />\n        </template>\n\n        <template v-slot:hint> 视频/音频 </template>\n\n        <template v-slot:append>\n          <q-icon name=\"cloud_upload\" size=\"md\" />\n        </template>\n      </q-file>\n\n      <!-- 任务标题：不可重复 -->\n      <q-input\n        outlined\n        clearable\n        clear-icon=\"close\"\n        color=\"purple\"\n        v-model=\"title\"\n        label=\"任务标题\"\n        hint=\"请勿重复使用同一标题\"\n        error-message=\"×标题已存在，请重新命名\"\n        :error=\"isRepeted\"\n        name=\"title\"\n        lazy-rules\n        bottom-slots\n        id=\"task-title\"\n        dense\n        autocomplete=\"off\"\n      >\n        <template v-slot:before>\n          <q-icon name=\"title\" size=\"lg\" />\n        </template>\n      </q-input>\n\n      <!-- 任务标签：可多选 -->\n      <q-select\n        outlined\n        clearable\n        clear-icon=\"close\"\n        v-model=\"modelMultiple\"\n        use-input\n        use-chips\n        multiple\n        counter\n        max-values=\"2\"\n        hint=\"Max 2 selections\"\n        input-debounce=\"0\"\n        @new-value=\"createValue\"\n        :options=\"filterOptions\"\n        @filter=\"filterFn\"\n        color=\"purple-6\"\n        transition-show=\"jump-up\"\n        transition-hide=\"jump-up\"\n        label=\"-- 任务标签 --\"\n        name=\"videoLabel[]\"\n        dense\n      >\n        <template v-slot:before>\n          <q-icon name=\"more\" size=\"lg\" />\n        </template>\n      </q-select>\n\n      <!-- 任务小组名单 -->\n      <q-select\n        outlined\n        clearable\n        clear-icon=\"close\"\n        v-model=\"tagName\"\n        :options=\"newTagList\"\n        @filter=\"filterFn\"\n        color=\"purple-6\"\n        label=\"-- 小组名单 --\"\n        name=\"tag\"\n        dense\n        hint=\"非必需选项\"\n      >\n        <template v-slot:before>\n          <q-icon name=\"group\" size=\"lg\" />\n        </template>\n\n        <template v-slot:no-option v-if=\"newTagList.length == 0\">\n          <q-item>\n            <q-item-section class=\"text-italic text-grey\">\n              请上传用户小组名单\n            </q-item-section>\n          </q-item>\n        </template>\n      </q-select>\n\n      <!-- 任务截止时间 -->\n      <div class=\"row q-my-sm\">\n        <div class=\"q-field__before q-field__marginal row no-wrap\">\n          <q-icon name=\"schedule\" size=\"lg\" />\n        </div>\n        <div\n          class=\"q-field__inner relative-position col self-stretch column justify-center\"\n        >\n          <el-date-picker\n            v-model=\"deadline\"\n            type=\"datetime\"\n            placeholder=\"选择任务截止时间\"\n            name=\"deadline\"\n            value-format=\"yyyy-MM-dd HH:mm:SS\"\n            size=\"large\"\n          >\n          </el-date-picker>\n        </div>\n      </div>\n\n      <!-- 任务最少字数限制 -->\n      <q-input\n        standout=\"bg-purple-5 text-white\"\n        v-model=\"words\"\n        label=\"最少字数限制\"\n        clearable\n        clear-icon=\"close\"\n        dense\n        name=\"words\"\n      >\n        <template v-slot:before>\n          <q-icon name=\"spellcheck\" size=\"lg\" />\n        </template>\n      </q-input>\n\n      <!-- 进入任务相关验证码 -->\n      <el-checkbox v-model=\"checked\" @change=\"initAuthcode\">验证码</el-checkbox>\n      <q-input\n        dense\n        v-if=\"checked\"\n        color=\"purple-6\"\n        v-model=\"authcode\"\n        clearable\n        clear-icon=\"close\"\n        placeholder=\"填写验证码\"\n        name=\"authcode\"\n      >\n        <template v-slot:prepend>\n          <q-icon name=\"vpn_key\" />\n        </template>\n      </q-input>\n\n      <br />\n      <el-switch\n        v-model=\"isManual\"\n        active-text=\"手动确认成绩\"\n        inactive-text=\"自动确认成绩\"\n        name=\"isManual\"\n      >\n      </el-switch>\n\n      <!-- 提交所有信息按钮 -->\n      <div class=\"row justify-center q-mt-md\">\n        <q-btn\n          push\n          icon-right=\"mdi-send-check\"\n          label=\"Submit\"\n          type=\"submit\"\n          color=\"purple-5\"\n          :disabled=\"titleExist\"\n        />\n      </div>\n    </q-form>\n  </div>\n</template>\n\n<script>\n\nimport { QSpinnerFacebook } from \"quasar\";\n\nconst stringOptions = [\n  \"Economy\",\n  \"Science\",\n  \"Politics\",\n  \"Literature\",\n  \"Coronavirus\",\n  \"Health\",\n  \"Nature\",\n  \"Sports\",\n  \"Celebrity\",\n  \"Others\",\n];\n\nexport default {\n  name: \"task\",\n  data() {\n    return {\n      publisher: \"\",\n      authcode: \"\", //验证码\n      checked: false,\n      deadline: \"\", //截止日期\n      title: \"\",\n      titleList: [],\n      titleExist: false,\n      accountList: [],\n      multiple: false,\n      newTagList: [],\n      status: \"\",\n      tagName: \"\",\n      api: \"/api\",\n      words: 0,\n      modelMultiple: [],\n      filterOptions: stringOptions,\n      fileName: null, // 所上传的文件名\n      isManual: false, //是否自动计算分数\n    };\n  },\n  computre: {\n    // 检测任务标题是否重名\n    isRepeted() {\n      for (var i = 0; i < this.titleList.length; i++) {\n        if (this.title == this.titleList[i]) {\n          return true; //重名则无法提交\n        }\n      }\n    },\n  },\n  methods: {\n    getTitleList() {\n      //连接数据库，获取数据库中所有的title\n      this.$axios.get(this.api + \"/titleList\").then((res) => {\n        var data = res.data;\n        for (var i = 0; i < data.length; i++) {\n          this.titleList.push(data[i].title);\n        }\n        // console.log(\"已发布任务标题\", this.titleList);\n      });\n    },\n    getTagList() {\n      //连接数据库，获取数据库中属于当前用户的tag列表\n      this.$axios\n        .get(this.api + \"/tagList?owner=\" + this.publisher)\n        .then((res) => {\n          var tagList = res.data;\n          // console.log(\"Tag数据表\", tagList);\n          this.newTagList = tagList.map((item) => {\n            return item.tagName;\n          });\n          // console.log(\"已发布任务小组标签\", this.newTagList)\n        });\n    },\n    getStatus() {\n      //连接数据库，获取数据库中属于当前用户的status\n      this.$axios\n        .get(this.api + \"/profile?name=\" + this.publisher)\n        .then((res) => {\n          this.status = res.data[0].status;\n        });\n    },\n    initAuthcode() {\n      this.authcode = \"\"; //清空验证码\n    },\n    // 把自定义标签暂时放到展示列表里，刷新不保留\n    createValue(val, done) {\n      if (val.length > 0) {\n        if (!stringOptions.includes(val)) {\n          stringOptions.push(val);\n        }\n        done(val, \"toggle\");\n      }\n    },\n    // 筛选任务标签\n    filterFn(val, update) {\n      update(() => {\n        if (val === \"\") {\n          this.filterOptions = stringOptions;\n        } else {\n          const needle = val.toLowerCase();\n          this.filterOptions = stringOptions.filter(\n            (v) => v.toLowerCase().indexOf(needle) > -1\n          );\n        }\n      });\n    },\n    // 提交时判断任务标题是否重复来决定是否符合发布要求\n    onSubmit(evt) {\n      let _this = this;\n      var findTitleIndex = this.titleList.indexOf(\n        this.titleList.find((element) => element == this.title)\n      );\n      if (findTitleIndex != -1 || this.title == \"\") {\n        _this.$q.notify({\n          color: \"red-5\",\n          textColor: \"white\",\n          timeout: 3000,\n          icon: \"sentiment_very_dissatisfied\",\n          message: \"发布失败！请再次检查信息是否完整准确！\",\n        });\n      } else {\n        this.titleExist = false;\n        evt.target.submit();\n        // 提交时的加载过渡动画\n        this.$q.loading.show({\n          spinner: QSpinnerFacebook,\n          spinnerColor: \"grey-2\",\n          spinnerSize: 60,\n          backgroundColor: \"purple-5\",\n        });\n        // hiding in 3s\n        this.timer = setTimeout(() => {\n          this.$q.loading.hide();\n          this.timer = void 0;\n        }, 3000);\n      }\n    },\n    // 网络加载完后，便结束加载动画，返回profile默认页面\n    beforeDestroy() {\n      if (this.timer !== void 0) {\n        clearTimeout(this.timer);\n        this.$q.loading.hide();\n      }\n    },\n  },\n  // 去掉任务标题重名时所在div的class中的“row”，主要为css服务\n  mounted() {\n    var taskTitle = document.querySelector(\n      \"#app > div > div.content-box > div > div.q-splitter__panel.q-splitter__after.col > div > div > div > div > form > label.q-field.q-validation-component.row.no-wrap.items-start.q-input.q-field--outlined.q-field--labeled.q-field--dense.q-field--with-bottom > div.q-field__inner.relative-position.col.self-stretch.column.justify-center > div.q-field__bottom.row.items-start.q-field__bottom--animated\"\n    );\n    taskTitle.className = taskTitle.className.replace(/\\brow\\b/g, \"\");\n  },\n  created() {\n    this.publisher = sessionStorage.getItem(\"userName\");\n    this.getTitleList();\n    this.getTagList();\n    this.getStatus();\n  },\n};\n</script>\n\n<style>\n.q-chip {\n  background-color: #fff3e6;\n}\n.q-field--auto-height .q-field__control,\n.q-field--auto-height .q-field__native,\n.q-field--filled .q-field__control,\n.q-field__control.relative-position.row.no-wrap {\n  width: 100%;\n}\ndiv.q-menu.q-position-engine.scroll {\n  z-index: 99999;\n  max-height: 300px !important;\n}\n.el-date-editor.el-input,\n.el-date-editor.el-input__inner {\n  width: 100%;\n}\n.el-input__icon:after {\n  vertical-align: top;\n}\n</style>"],"sourceRoot":"src/components/profile"}]}