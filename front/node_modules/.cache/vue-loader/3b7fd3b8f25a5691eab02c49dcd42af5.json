        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AvatarOrg from \"../basic/Avatar\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      api: \"score\",\r\n      name: \"\",\r\n      myRank: \"\",\r\n      myScore: \"\",\r\n      winnerName: \"\",\r\n      winnerScore: \"\",\r\n      team: [],\r\n    };\r\n  },\r\n  components: {\r\n    AvatarOrg,\r\n  },\r\n  created() {\r\n    this.name = sessionStorage.getItem(\"userName\");\r\n    this.$axios.get(this.api + \"/coin\").then((res) => {\r\n      this.team = res.data;\r\n      this.pushRank();\r\n    });\r\n  },\r\n\r\n  methods: {\r\n    pushRank() {\r\n      const randomEmoji = () => {\r\n        const emojis = [\"üëè\", \"üëç\", \"üôå\", \"ü§©\", \"üî•\", \"‚≠êÔ∏è\", \"üèÜ\", \"üíØ\"];\r\n        let randomNumber = Math.floor(Math.random() * emojis.length);\r\n        return emojis[randomNumber];\r\n      };\r\n      this.team.forEach((member) => {\r\n        if (member.rank <= 10) {\r\n          let newRow = document.createElement(\"li\");\r\n          var oUl = document.getElementsByClassName(\"c-list list\");\r\n          newRow.classList = \"c-list__item\";\r\n          newRow.innerHTML = `\r\n\t\t<div class=\"c-list__grid box\">\r\n\t\t\t<div class=\"c-flag c-place u-bg--transparent\">${member.rank}</div>\r\n      <div class=\"c-media\">\r\n      <avatar-org></avatar-org>\t\r\n\t\t\t\t<div class=\"c-media__content\">\r\n\t\t\t\t\t<div class=\"c-media__title\">${member.name}</div>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t\t<div class=\"u-text--right c-kudos\">\r\n\t\t\t\t<div class=\"u-mt--8\">\r\n\t\t\t\t\t<strong>${member.totalCoins}</strong> ${randomEmoji()}\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t</div>\r\n  `;\r\n          if (member.rank === 1) {\r\n            this.winnerName = member.name;\r\n            this.winnerScore = member.totalCoins;\r\n          }\r\n          if (this.name == member.name) {\r\n            this.myRank = member.rank;\r\n            this.myScore = member.totalCoins;\r\n          }\r\n          oUl[0].appendChild(newRow);\r\n        }\r\n      });\r\n    },\r\n  },\r\n  watch: {\r\n    $route(to, from) {\r\n      //Âà∑Êñ∞ÂèÇÊï∞ÊîæÂà∞ËøôÈáåÈáåÈù¢ÂéªËß¶ÂèëÂ∞±ÂèØ‰ª•Âà∑Êñ∞Áõ∏ÂêåÁïåÈù¢‰∫Ü\r\n      this.pushRank();\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}